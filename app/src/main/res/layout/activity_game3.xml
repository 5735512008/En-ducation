<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.nam.en_ducation.Game3Activity"
    android:background="@drawable/bgqq">

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        android:src="@drawable/picans01"
        android:layout_above="@+id/imageView18"
        android:layout_alignLeft="@+id/imageView17"
        android:layout_alignStart="@+id/imageView17"
        android:layout_marginBottom="15dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drop4"
        android:src="@drawable/dropans1"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/drop1"
        android:layout_toEndOf="@+id/drop1" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView17"
        android:src="@drawable/pic01"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/drop1"
        android:layout_toEndOf="@+id/drop1" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drag1"
        android:src="@drawable/dropflase1"
        android:layout_below="@+id/imageView16"
        android:layout_toLeftOf="@+id/drop2"
        android:layout_marginRight="14dp"
        android:layout_marginEnd="14dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drag2"
        android:src="@drawable/droptrue1"
        android:layout_alignTop="@+id/drag1"
        android:layout_toRightOf="@+id/drag1"
        android:layout_toEndOf="@+id/drag1" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drag3"
        android:src="@drawable/dropflase1a"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:layout_alignTop="@+id/drag2"
        android:layout_toRightOf="@+id/drag2"
        android:layout_toEndOf="@+id/drag2" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView18"
        android:src="@drawable/ans"
        android:layout_above="@+id/drop4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="10dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drop1"
        android:src="@drawable/drop1"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drop2"
        android:src="@drawable/drop1c"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/drop4"
        android:layout_toEndOf="@+id/drop4" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drop5"
        android:src="@drawable/drop1d"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/drag2"
        android:layout_toEndOf="@+id/drag2" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drop6"
        android:src="@drawable/drop1e"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/imageView17"
        android:layout_toEndOf="@+id/imageView17" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView16"
        android:src="@drawable/ansselect"
        android:layout_below="@+id/imageView17"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
