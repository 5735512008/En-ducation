<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.nam.en_ducation.Game3bActivity"
    android:background="@drawable/bgqq">

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView18"
        android:layout_centerHorizontal="true"
        android:background="@null"
        android:src="@drawable/picans03" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drop2"
        android:src="@drawable/dropans2"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/imageView15"
        android:layout_alignStart="@+id/imageView15" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drop3"
        android:src="@drawable/drop3a"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/drop2"
        android:layout_toEndOf="@+id/drop2" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView17"
        android:src="@drawable/pic03"
        android:layout_marginTop="18dp"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/imageView15"
        android:layout_alignStart="@+id/imageView15" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drag1"
        android:src="@drawable/dropflase3"
        android:layout_alignTop="@+id/imageView15"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drag2"
        android:src="@drawable/dropflase3a"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp"
        android:layout_alignTop="@+id/imageView15"
        android:layout_toRightOf="@+id/drag1"
        android:layout_toEndOf="@+id/drag1" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drag3"
        android:src="@drawable/droptrue3"
        android:layout_alignTop="@+id/imageView15"
        android:layout_toRightOf="@+id/drag2"
        android:layout_toEndOf="@+id/drag2"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView18"
        android:src="@drawable/ans"
        android:layout_above="@+id/drop2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView16"
        android:src="@drawable/ansselect"
        android:layout_above="@+id/imageView15"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>
