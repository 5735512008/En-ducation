<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.nam.en_ducation.Game3hActivity" android:background="@drawable/bgqq">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drop4"
        android:src="@drawable/dropans1"
        android:layout_alignTop="@+id/drop6"
        android:layout_toRightOf="@+id/imageView15"
        android:layout_toEndOf="@+id/imageView15" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView17"
        android:src="@drawable/pic09"
        android:layout_marginBottom="15dp"
        android:layout_above="@+id/drag1"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drag1"
        android:src="@drawable/droptrue9"
        android:layout_alignTop="@+id/drag2"
        android:layout_toRightOf="@+id/drag2"
        android:layout_toEndOf="@+id/drag2" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drag2"
        android:src="@drawable/dropflase9"
        android:layout_alignTop="@+id/drag3"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drag3"
        android:src="@drawable/dropflase9a"
        android:layout_above="@+id/imageView18"
        android:layout_toLeftOf="@+id/drag2"
        android:layout_toStartOf="@+id/drag2" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView18"
        android:src="@drawable/ans"
        android:layout_above="@+id/drop1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drop1"
        android:src="@drawable/drop9"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="38dp" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drop2"
        android:src="@drawable/drop9a"
        android:layout_alignTop="@+id/drop1"
        android:layout_toRightOf="@+id/drop1"
        android:layout_toEndOf="@+id/drop1" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drop5"
        android:src="@drawable/drop3b"
        android:layout_alignTop="@+id/drop2"
        android:layout_toLeftOf="@+id/drop6"
        android:layout_toStartOf="@+id/drop6" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/drop6"
        android:src="@drawable/drop9c"
        android:layout_alignTop="@+id/drop5"
        android:layout_toLeftOf="@+id/drop4"
        android:layout_toStartOf="@+id/drop4" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView16"
        android:src="@drawable/ansselect"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        android:src="@drawable/picans09"
        android:layout_alignTop="@+id/drag1"
        android:layout_alignLeft="@+id/imageView17"
        android:layout_alignStart="@+id/imageView17" />

</RelativeLayout>
