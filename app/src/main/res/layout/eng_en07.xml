<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.nam.en_ducation.EngEn07"
    android:background="@drawable/bgqq">
    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="19dp"
        android:text="ฝึกอ่านคำศัพท์หมวดครอบครัว"
        android:textColor="@color/colorAccent"
        android:textSize="25dp" />
    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="39dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="65dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:orientation="vertical"
            android:weightSum="1">

            <LinearLayout
                android:layout_width="311dp"
                android:layout_height="151dp"
                android:orientation="horizontal"
                android:weightSum="1">

                <ImageView
                    android:id="@+id/imageView1"
                    android:layout_width="109dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/c01"
                    android:layout_weight="0.40" />

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="103dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/c02"
                    android:layout_weight="0.46" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="311dp"
                android:layout_height="148dp"
                android:orientation="horizontal"
                android:weightSum="1">

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="138dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/c03"
                    android:layout_weight="0.28" />

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="132dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.46"
                    app:srcCompat="@drawable/c04" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="311dp"
                android:layout_height="154dp"
                android:orientation="horizontal"
                android:weightSum="1">

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="152dp"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/c05" />

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/c06" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="311dp"
                android:layout_height="152dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/c07" />

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/c08" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="311dp"
                android:layout_height="158dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/c09" />

                <ImageView
                    android:id="@+id/imageView10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/c10" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="311dp"
                android:layout_height="165dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/c11" />

                <ImageView
                    android:id="@+id/imageView12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/c12" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="311dp"
                android:layout_height="158dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/c13" />

                <ImageView
                    android:id="@+id/imageView14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/c14" />


            </LinearLayout>
            <LinearLayout
                android:layout_width="311dp"
                android:layout_height="161dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/c15" />

                <ImageView
                    android:id="@+id/imageView16"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/c16" />


            </LinearLayout>
            <LinearLayout
                android:layout_width="311dp"
                android:layout_height="155dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView17"
                    android:layout_width="match_parent"
                    android:layout_height="166dp"
                    app:srcCompat="@drawable/c17" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</RelativeLayout>
